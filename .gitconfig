[alias]
	d = difftool
	lga = log --branches --remotes --tags --graph --decorate --oneline
	lg = log --graph --decorate --oneline
	lgf = log --graph --decorate --oneline --first-parent
	s = status
	ch = checkout
	f = fetch --all
	c = commit
	au = add -u :/
[color]
	diff = auto
	status = auto
	branch = auto
	interactive = true
[core]
	editor = sublime_text.exe -n -w
	autocrlf = true
	symlinks = false
	fscache = true
[diff]
	tool = meld
[diff "astextplain"]
	textconv = astextplain
[help]
	format = html
[merge]
	tool = meld
[push]
	default = upstream
[sendemail]
	smtpserver = /bin/msmtp.exe
[user]
	email = jchchan91@gmail.com
	name = Jonathan Chan
